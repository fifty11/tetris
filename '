#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>
#include <sys/types.h>
#include "stdbool.h"
#include "tetris.h"

char game;

int main(int argc, char** argv)
{
	// constants:
	struct Tetrinos
	{
		uint16_t L; // 
		uint16_t BackwardsL; // 
		uint16_t Bar; // 
		uint16_t Square; // 
		uint16_t Pyramid; // 
	} tetrinos;
		
	tetrinos.L=0x6440; // 
	tetrinos.BackwardsL=0x6220; // 
	tetrinos.Bar=0x4444; // 
	tetrinos.Square=0x6600; // 
	tetrinos.Pyramid=0xE400; // 
	
	// vars
	int seed;

	uint16_t tetrinoList[4];

	game=true;

	// game
	if(argc > 1)
		seed=atoi(argv[0]);

	for(int tetrino=0; tetrino<5; ++tetrino)
	{
		for(int i=0; i<16; ++i)
		{
			if((*tetrinos[tetrino])&(1<<i))
				printf("1 ");
			else
				printf("0 ");
			if(!((i+1)%4))
				printf("\n");
		}
		printf("\n\n");
	}
		return 0;
}
